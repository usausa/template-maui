<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:s="http://smart/maui"
             xmlns:shell="clr-namespace:Template.MobileApp.Shell"
             xmlns:module="clr-namespace:Template.MobileApp.Modules.UI"
             x:Class="Template.MobileApp.Modules.UI.UIMenuView"
             x:DataType="{x:Type module:UIMenuViewModel}"
             s:BindingContextResolver.Type="{x:Type module:UIMenuViewModel}"
             shell:ShellProperty.Title="UI"
             shell:ShellProperty.Function1Text="Back"
             shell:ShellProperty.Function1Enabled="True">

    <Grid Style="{StaticResource MenuGrid}"
          RowDefinitions="*,*,*,*,*,*,*,*,*">
        <Button Grid.Row="0"
                Style="{StaticResource MenuButton}"
                IsEnabled="False"
                Text="Login" />
        <Button Grid.Row="1"
                Style="{StaticResource MenuButton}"
                IsEnabled="False"
                Text="Profile" />
        <Button Grid.Row="2"
                Style="{StaticResource MenuButton}"
                IsEnabled="False"
                Text="Portal" />
        <Button Grid.Row="3"
                Style="{StaticResource MenuButton}"
                IsEnabled="False"
                Text="Dock" />
        <Button Grid.Row="4"
                Style="{StaticResource MenuButton}"
                IsEnabled="False"
                Text="Calendar" />
        <Button Grid.Row="5"
                Style="{StaticResource MenuButton}"
                IsEnabled="False"
                Text="Grid" />
        <Button Grid.Row="6"
                Style="{StaticResource MenuButton}"
                IsEnabled="False"
                Text="Timeline" />
        <Button Grid.Row="7"
                Style="{StaticResource MenuButton}"
                IsEnabled="False"
                Text="Gauge" />
        <Button Grid.Row="8"
                Style="{StaticResource MenuButton}"
                IsEnabled="False"
                Text="Radar" />
    </Grid>

</ContentView>
